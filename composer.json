{
    "name": "meridienclube/laravel-user",
    "version": "dev-master",
    "description": "Complete user pack for laravel",
    "keywords": [
        "user",
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "type": "package",
    "authors": [
        {
            "name": "Rafael Zingano",
            "email": "confrariaweb@gmail.com"
        }
    ],
    "require": {
        "meridienclube/laravel-datatable": "dev-master",
        "meridienclube/laravel-entrust": "dev-master",
        "meridienclube/laravel-file": "dev-master",
        "meridienclube/laravel-historic": "dev-master",
        "meridienclube/laravel-option": "dev-master",
        "meridienclube/laravel-select2": "dev-master",
        "meridienclube/laravel-vendor": "dev-master",
        "laravelcollective/html": ">=6.0",
        "staudenmeir/eloquent-has-many-deep" : ">=1.11"
    },
    "autoload": {
        "psr-4": {
            "ConfrariaWeb\\User\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ConfrariaWeb\\User\\Providers\\UserEventServiceProvider",
                "ConfrariaWeb\\User\\Providers\\UserServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev"
}
